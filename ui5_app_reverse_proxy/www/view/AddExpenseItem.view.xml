<mvc:View	controllerName="linklaters.com.customexpensesapp.view.AddExpenseItem"	xmlns:mvc="sap.ui.core.mvc"	xmlns:l="sap.ui.layout"	xmlns:f="sap.ui.layout.form"	xmlns:c="sap.ui.core"	xmlns="sap.m"><Page		class="sapUiFioriObjectPage"		title="{employeeModel>/ExpenseYear} / {employeeModel>/ExpenseMonth}">		<l:Grid			defaultSpan="L12 M12 S12"			width="auto">			<l:content>				<f:SimpleForm					id="idAddExpenseItemForm"					minWidth="800"					maxContainerCols="2"					editable="true"					layout="ResponsiveGridLayout"					title=" Status : New "					labelSpanL="3"					labelSpanM="3"					emptySpanL="4"					emptySpanM="4"					columnsL="1"					columnsM="1"					class="editableForm">					<f:content>                        <c:Title text="Status : New" />                        <Label text="Description" />                        <Input value="{newExpenseItem>/Detail/Description}" />						<Label text="Client" />						<Input value="{newExpenseItem>/Detail/Client}" />						<Label text="Type" />						<Select							id="idSelectType"							items="{/ExpenseTypeSet}"							width="100%">							<c:Item text="{Description}" />						</Select>						<Label text="Amount" />						<Input value="{newExpenseItem>/Detail/Amount}" />												<Label text="Location" />						<Input value="{newExpenseItem>/Detail/Location}" />                        <Label text="Date" />						<DateTimeInput							type="Date"							value="{newExpenseItem>/Detail/Date}" />							</f:content>				</f:SimpleForm>			</l:content>		</l:Grid><footer>			<Bar>				<contentRight>					<Button text="Save" type="Emphasized" press="onSave" />					<Button text="Cancel" press="onCancel" />				</contentRight>			</Bar>		</footer>	</Page></mvc:View>